@model ReportsView
<h4 style="color:#000000;">Reports.</h4>
<div class="container" style="background-color:#ffffff">
	<section>
		@foreach(var report in Model.Reports)
		{
			<div style="background-color:#ffffff; box-shadow: 5px 10px 8px #888888;">
				<a href="@string.Format("https://localhost/ReportServer/Pages/ReportViewer.aspx?/Reports/{0}&rs:embed=true", report.Name)">@report.Name</a>
			</div>
		}
	</section>
</div>
