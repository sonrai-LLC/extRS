@model DataSourcesView

&nbsp;
<div class="container">
    <section>
        @foreach (var dataSource in Model.DataSources)
        {
            <div id=@("dialog" + dataSource.Id) class="dialog" style="display: none">
            </div>
            <div class="bg-dark" style="box-shadow: 2.5px 5px 4px #888888;">
                <span id="@dataSource.Id" class="nav_link" style="float:right" onclick="asyncManageResourceModal('@dataSource.Id');">
                    <a href="#">
                        ...
                    </a>
                </span><a href="@dataSource.Uri" class="nav_link" @Model.OpenInLinksNewTab ? "target=_blank"><i class="bx bx-cube"></i><span class="nav_name">@dataSource.Name</span></a>
            </div>
        }
    </section>
</div>
